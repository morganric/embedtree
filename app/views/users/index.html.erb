<div class="container">
  <div class="row col-md-8">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
<div class="col-md-4">
  <div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Views</h3>
  </div>
  <div class="panel-body">
    <%= @views %>
  </div>
</div>

  <div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title">Posts</h3>
  </div>
  <div class="panel-body">
    <%= @posts.count %>
  </div>
</div>

</div>

</div>


